    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon app",
    "babel": "babel server/src -d server/lib",
    "build:client": "webpack",
    "build:server": "babel ./server/src -d ./server/lib",
    "dev": "nodemon --watch server/src --exec \"npm run watch:client && npm run babel && npm run start:server\"",
    "dev:new": "concurrently -k \"npm run watch:client\" \"npm run watch:server\" \"nodemon --watch server/lib/ --delay 1\"",
    "prepare": "npm run build:server",
    "start:dev": "npm run build:client && npm run build:server && npm run start:server",
    "start:server": "node server/lib/index.js",
    "watch:client": "webpack --watch",
    "watch:server": "babel ./server/src -w -d ./server/lib"
  },
